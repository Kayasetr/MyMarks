<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.firebase.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyTranslucentTheme">
        <activity
            android:name=".SettingsActivity"
            android:label=""/>
        <activity
            android:name=".AboutAppActivity"
            android:label="" />
        <activity
            android:name=".AdministratorActivity"
            android:label="" />
        <activity
            android:name=".ManagerShowOtherGroups"
            android:label=""
            android:theme="@style/LightTheme.SlidrActivityTheme" />
        <activity
            android:name=".UpdateCheck"
            android:label="" />
        <activity
            android:name=".StudentActivity"
            android:label="" />
        <activity
            android:name=".ManagerActivity"
            android:label="" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>